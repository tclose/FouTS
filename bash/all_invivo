#!/usr/bin/env bash
invivo_sampling.py --dataset donald/images/corpus_callosum.150.no-iso.mif --seed_positions donald/images/corpus_callosum.150.seed.txt --intensity_estimate donald/intensity.150.txt --like_noise_map donald/images/corpus_callosum.150.noise.mif --num_runs 5
invivo_sampling.py --dataset heath/images/corpus_callosum.150.no-iso.mif  --seed_positions heath/images/corpus_callosum.150.seed.txt  --intensity_estimate heath/intensity.150.txt --like_noise_map heath/images/corpus_callosum.150.noise.mif --num_runs 5
invivo_sampling.py --dataset lisa/images/corpus_callosum.150.no-iso.mif   --seed_positions lisa/images/corpus_callosum.150.seed.txt   --intensity_estimate lisa/intensity.150.txt --like_noise_map lisa/images/corpus_callosum.150.noise.mif --num_runs 5
invivo_sampling.py --dataset donald/images/corpus_callosum.60.no-iso.mif  --seed_positions donald/images/corpus_callosum.60.seed.txt  --intensity_estimate donald/intensity.60.txt --like_noise_map donald/images/corpus_callosum.60.noise.mif --num_runs 5
invivo_sampling.py --dataset heath/images/corpus_callosum.60.no-iso.mif   --seed_positions heath/images/corpus_callosum.60.seed.txt   --intensity_estimate heath/intensity.60.txt --like_noise_map heath/images/corpus_callosum.60.noise.mif --num_runs 5
invivo_sampling.py --dataset lisa/images/corpus_callosum.60.no-iso.mif    --seed_positions lisa/images/corpus_callosum.60.seed.txt    --intensity_estimate lisa/intensity.60.txt --like_noise_map lisa/images/corpus_callosum.60.noise.mif --num_runs 5
invivo_sampling.py --dataset donald/images/corpus_callosum.20.no-iso.mif  --seed_positions donald/images/corpus_callosum.20.seed.txt  --intensity_estimate donald/intensity.20.txt --like_noise_map donald/images/corpus_callosum.20.noise.mif --num_runs 5

invivo_sampling.py --dataset donald/images/fornix.150.no-iso.mif --seed_positions donald/fornix.150.seed.txt --intensity_estimate donald/intensity.150.txt --like_noise_map donald/images/fornix.150.noise.mif --num_runs 5 --seed_pos_stddev 0.1 --num_iterations 25000 --sample_period 1000 --split
invivo_sampling.py --dataset heath/images/fornix.150.no-iso.mif  --seed_positions heath/fornix.150.seed.txt  --intensity_estimate heath/intensity.150.txt --like_noise_map heath/images/fornix.150.noise.mif --num_runs 5 --seed_pos_stddev 0.1 --num_iterations 25000 --sample_period 1000 --split
invivo_sampling.py --dataset lisa/images/fornix.150.no-iso.mif   --seed_positions lisa/fornix.150.seed.txt   --intensity_estimate lisa/intensity.150.txt --like_noise_map lisa/images/fornix.150.noise.mif --num_runs 5 --seed_pos_stddev 0.1 --num_iterations 25000 --sample_period 1000 --split
invivo_sampling.py --dataset donald/images/fornix.60.no-iso.mif  --seed_positions donald/fornix.60.seed.txt  --intensity_estimate donald/intensity.60.txt --like_noise_map donald/images/fornix.60.noise.mif --num_runs 5 --seed_pos_stddev 0.1 --num_iterations 25000 --sample_period 1000 --split
invivo_sampling.py --dataset heath/images/fornix.60.no-iso.mif   --seed_positions heath/fornix.60.seed.txt   --intensity_estimate heath/intensity.60.txt --like_noise_map heath/images/fornix.60.noise.mif --num_runs 5 --seed_pos_stddev 0.1 --num_iterations 25000 --sample_period 1000 --split
invivo_sampling.py --dataset lisa/images/fornix.60.no-iso.mif    --seed_positions lisa/fornix.60.seed.txt    --intensity_estimate lisa/intensity.60.txt --like_noise_map lisa/images/fornix.60.noise.mif --num_runs 5 --seed_pos_stddev 0.1 --num_iterations 25000 --sample_period 1000 --split
invivo_sampling.py --dataset donald/images/fornix.20.no-iso.mif  --seed_positions donald/fornix.20.seed.txt  --intensity_estimate donald/intensity.20.txt --like_noise_map donald/images/fornix.20.noise.mif --num_runs 5 --seed_pos_stddev 0.1 --num_iterations 25000 --sample_period 1000 --split
